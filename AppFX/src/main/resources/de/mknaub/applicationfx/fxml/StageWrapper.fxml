<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="root" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.mknaub.applicationfx.ctrl.StageWrapperCtrl">
   <children>
      <HBox fx:id="headerBox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="appfx-scene-dialog-header">
         <children>
            <HBox fx:id="dlgHeaderContainer" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <children>
                  <Label fx:id="titleLbl" text="Dialog title" />
               </children>
               <padding>
                  <Insets left="5.0" right="5.0" />
               </padding>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </HBox>
            <Button fx:id="minimizeBtn" focusTraversable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false">
               <graphic>
                  <SVGPath content="M6 16l12 0l0 2l-12 0z" />
               </graphic>
            </Button>
            <Button fx:id="maximizeBtn" focusTraversable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false">
               <graphic>
                  <SVGPath content="M6 6 l12 0 l0 12 l-12 0 l0 -12 M6 7 l12 0" fill="TRANSPARENT" />
               </graphic>
            </Button>
            <Button fx:id="closeBtn" focusTraversable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false">
               <graphic>
                  <SVGPath content="M18 6l-5 5 5 5-1 1-5-5-5 5-1-1 5-5-5-5 1-1 5 5 5-5z" />
               </graphic>
            </Button>
         </children>
      </HBox>
      <BorderPane fx:id="contentPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="appfx-scene-dialog-content" VBox.vgrow="ALWAYS" />
   </children>
   <styleClass>
      <String fx:value="scene" />
      <String fx:value="appfx-container" />
      <String fx:value="appfx-scene-dialog" />
   </styleClass>
   <stylesheets>
      <URL value="@../style/css/appfx-bright-theme.css" />
      <URL value="@../style/css/default-style.css" />
   </stylesheets>
</VBox>
